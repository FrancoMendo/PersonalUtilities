{
  "expo": {
    "name": "Stelar",
    "slug": "Stelar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/blackHoleIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashSpace.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 4000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/blackHoleIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.francomendo.Stelar"
    },
    "web": {
      "favicon": "./assets/images/blackHoleIcon.png"
    }
  }
}
